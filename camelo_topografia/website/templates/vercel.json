{
    "version": 2,
    "builds": [
      {
        "src": "your_project_name/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "your_project_name.settings",
      "DB_NAME": "your_database_name",
      "DB_USER": "your_database_user",
      "DB_PASSWORD": "your_database_password",
      "DB_HOST": "your_database_host"
    }
  }
  